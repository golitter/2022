#include <iostream>
#include <vector>
#include <string>
#include <algorithm>
using namespace std;

int main()
{
    vector<int> vea{1,2,3,4,5};
    auto it = vea.begin();
    while(it != vea.end()) {
        cout<<*it++<<" ";
    }
    /**
     * @brief 
     * 1 2 3 4 5；
     * NOTICE:
     * 不是 (*it)++;
     * BCOS:
     * 后置递增运算符优先级 高于 解引用运算符;
     *     优先级好像跟C不太一样
     * 等价于：
     * for(auto i: vea) {
     *      cout<<i<<" ";
     * }
     */
    return 0;
}
