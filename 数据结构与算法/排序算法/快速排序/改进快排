#include <iostream>
#include <vector>
#include <string>
#include <set>
#include <algorithm>
using namespace std;
int arr[1001];

// big --> small;
void mysort(int l, int r) {
	int i = l, j = r, mid;
	mid = arr[(l+r)/2];
	do {
		while(arr[i] > mid) ++i;
		while(arr[j] < mid) --j;
		if(i <= j) {
			swap(arr[i],arr[j]);
			++i;
			--j;
		}
	} while(i <= j);
	if(l < j) mysort(l, j);
	if(r > i) mysort(i, r);
}

int main()
{
	for(int i = 0; i <1001; ++i) arr[i] = i;
	int k = *find(arr+0, arr+1001, 34);
	cout<<k;
	mysort(0,1000);
	for(auto &i: arr) {
		cout<<i<<" ";
	}
	return 0;
}
