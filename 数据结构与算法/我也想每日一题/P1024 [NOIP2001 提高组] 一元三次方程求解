#include <bits/stdc++.h>

using namespace std;
double a,b,c,d;
double formula(double ans) {
    return a*ans*ans*ans + b*ans*ans + c*ans +d;
}

void pd(double L, double R) {
    double mid = (L + R)/2;
    if(formula(L) == 0) {printf("%.2lf ",L); return ;}
    if(formula(mid) == 0) {printf("%.2lf ",mid); return ;}
    if(formula(R) == 0) {printf("%.2lf ",R); return ;}
    if(R - L <= 0.001) {printf("%.2lf ",mid); return ;} // keep two decinals;
    if(formula(L)*formula(mid) < 0) {
        pd(L,mid);
    } else {
        pd(mid,R);
    }
}

int main()
{
    
    cin>>a>>b>>c>>d;
    double daoa,daob,daoc,daod;// these are my words of definition;
    daoa = 3*a; daob = 2*b; daoc = c; // I do not knew word of derivative, so use pinyin to named; 
    double formula_ = sqrt(daob*daob - 4*daoa*daoc); // NOTICE: MUST didtinguish your statement;
    double x1 = (-daob - formula_)/(2*daoa);// a not equal daoa; error there;
    double x2 = (-daob + formula_)/(2*daoa);
    if(x2 < x1) {
        swap(x1,x2);
    }
    // the first answer;
    pd(-100,x1);
    // second;
    pd(x1,x2);
    // third;
    pd(x2,100);
}
