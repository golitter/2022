#include <iostream>
#include <new>
#include <cstdio>
using namespace std;
typedef struct list{
    int val;
    struct list *next;
}List, *LinkList;
int main()
{
    LinkList head = new List;
    head->next = NULL;
    for(int i = 0 ; i < 34; i++) {
        LinkList q = new List;
        q->val = i;
        q->next = head->next;
        head->next = q;
    }
    head = head->next;
    while(head) {
        cout<<head->val;
        head = head->next;
    }
    cout<<endl;
    LinkList head2 = new List;
    head2->next = NULL;
    LinkList Tail = head2;
    for(int i = 0; i < 45; i++) {
        LinkList q1 = new List;
        q1->val = i;
        Tail->next = q1;
        Tail = q1;
    }
    head2 = head2->next;
    while(head2) {
        cout<<head2->val;
        head2 = head2->next;
    }
    cout<<endl;

    LinkList h1 = new List;
    h1 = NULL;
    for(int i = 1; i < 34; i++) {
        LinkList s1 = new List;
        s1->val = i;
        s1->next = h1;
        h1 = s1;
    }
    while(h1) {
        cout<<h1->val;
        h1 = h1->next;
    }
    cout<<endl;
    LinkList h2 = new List;
    h2 = NULL;
    LinkList res = new List;
    res->next = h2;
    LinkList w1 = res;
    for(int i = 0; i < 34; i++) {
        LinkList q = new List;
        q->val = i;
        w1->next = q;
        w1 = q;
    }
    h2 = res->next;
    while(h2) {
        cout<<h2->val;
        h2 = h2->next;
    }
}
