// this is recursion;
// about Sequential traversal of Binary Tree 
// preorder traversal and inorder traversal and postorder traversal;
// the question give inorder traversal and postorder traversal ;
// then seek its perorder traversal;
// what is sequential traversal ;
// preorder : root left subtree  right subtree
// inorder  : left subtree  root  right subtree;
// postorder: left subtree right subtree  root;
//notice : preorder traversal , not such ad ABCDEFG ;my error;
#include <bits/stdc++.h>
using namespace std;

void Traverse(string in, string after) {
    if(in.size() > 0) {
        char ch = after[after.size() - 1];
        cout<<ch;
        int k = in.find(ch);
        Traverse(in.substr(0,k), after.substr(0,k));
        Traverse(in.substr(k+1), after.substr(k,in.size() - 1 - k));
    }
}

int main()
{
    string preorder, inorder, postorder;
     cin>>inorder>>postorder;
     Traverse(inorder,postorder);

}
/*
  e.g.:
  inorder : DBEAFCG;
  postorder:DEBFGCA;
  in: DBE A FCG
  pt: DEB F GCA;
  find A is father root ;
  and also is  the last postorder pisition;
  so output A;
  then find at inoreder, DBE FCG
  corresponding pt     , DEB GCA;
  then recursion;
  
*/
