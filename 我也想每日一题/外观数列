#include <bits/stdc++.h>
using namespace std;
string Dserpd(string b) {
    int Len = b.size();
    string str;
    for(int i = 0; i < Len; ) {// error;
        int j = i;
        while(b[j] == b[i]) j++;
        str += j - i + '0';
        str += b[i];
        i = j;
    }
    return str;
}
string Dserp(string a, int times) {
    if(times == 1) return a;
    for(int i = 2; i <= times; i++) {
        a = Dserpd(a);
    }
    return a;
}


int main()
{
    int n;
    cin>>n;
    string Face;
    Face = "1";
    Face = Dserp(Face, n);
    for(int i = 0; i < Face.size(); i++) {
        cout<<Face[i];
    }
}
