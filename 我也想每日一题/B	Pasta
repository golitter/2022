#include <bits/stdc++.h>
using namespace std;
int N[1003];
int M[1003];
int judeN[1003];
int judeM[1003];
   int n,m;

int Find(int a) {
    for(int i = 0; i < n; i++) {
        if(a == N[i]) {
            N[i] = -1;
            return 1;
        }
    }
    return 0;
}

int main()
{
 
    cin>>n>>m;
    int jud;
    for(int i = 0 ; i < n; i++) {
        cin>>N[i];
    }
    for(int i = 0 ; i < m; i++) {
        cin>>M[i];jud = Find(M[i]);
        if(jud == 0) {
            cout<<"No"; return 0;
        }
    }
    cout<<"Yes";
}
