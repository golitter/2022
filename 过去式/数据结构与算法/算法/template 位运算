#include <iostream>
#include <vector>
#include <string>
#include <set>
#include <algorithm>
using namespace std;

// 位运算
void testBit() {
    int n = 10;
    for(int i = 3; i >= 0; --i) cout<<(n>>i & 1);
    cout<<endl;

}
// lowbit(x) 返回x的最后一位 
//int  lowbit(int x) { return x & -x;}
// -x = ~x + 1;
// x&1 == 1 奇数 or 偶数
// a^a = 0, a^0 = a;

int main()
{
    testBit();
    return 0;
}
