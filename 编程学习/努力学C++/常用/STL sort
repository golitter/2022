#include <bits/stdc++.h>
// STL sort && cmp  默认降序
/**
 * @brief 
 *      怎么理解cmp
 *  
 * 
 */

bool cmp1(int a, int b) {
    return a > b; // 降序    big ----> small
    // ret a < b;    升序    small <---- big;
    
    // 感觉可以将 (a<or>b) 比作箭头 由大数 指向 小数;
}


using namespace std;
struct st {
    string na;
    int ag;
    int sc;
}stu[1021];
bool cmp(st a, st b) {
    if(a.sc != b.sc) {
        return a.sc < b.sc;
    } else {
        if(a.na != b.na) {
            return a.na < b.na;
        } else {
            return a.ag < b.ag;
        }
    }
}
int main()
{
    int n;
    cin>>n;
    int N = n;
    while(n--) cin>>stu[n].na>>stu[n].ag>>stu[n].sc;
    sort(stu+0, stu+N, cmp);
    for(int i = 0; i < N; i++) {
        cout<<stu[i].na<<" "<<stu[i].ag<<" "<<stu[i].sc<<endl;
    }
}
