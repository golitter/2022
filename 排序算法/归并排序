#include<bits/stdc++.h>
using namespace std;  
int a[7]={0,3,6,13,31,9,27};
int za[7];
void merge(int l,int r,int mid)
{
  int i,j,p;
  i = l; j = mid; p = l;
  while(i<mid && j<=r) {
    if(a[i]<a[j]) za[p++] = a[i++];
    else za[p++] = a[j++];
  }
  while(i<mid) za[p++] = a[i++];
  while(j<=r) za[p++] = a[j++];
  p = 1;i = 1;
  while(p<=r)
  a[i++] = za[p++];
}
void merge_sort(int l,int r)
{
  if(l<r) {
    int mid=(l+r)/2;
    merge_sort(l,mid);
    merge_sort(mid+1,r);
    merge(l,r,mid+1);
  }
}
int main()
{
  int n = 6;
  merge_sort(1,6);

  for(int i=1;i<=n;i++) cout<<za[i]<<endl;
}

